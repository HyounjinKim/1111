package ex02;

import java.util.Scanner;

public class Ex08 {
	// 숫자 2개를 입력받아 더한 값 출력
	// scanner 안에 있는 함수
	// nextline 문자열을 받는다.
	// nextInt 정수를 받는다.
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("숫자 입력");
		int num1 = scan.nextInt();
		System.out.println("숫자 입력");
		int num2 = scan.nextInt();
		
		System.out.println("결과 : " + (num1 + num2));
		
	}

}
