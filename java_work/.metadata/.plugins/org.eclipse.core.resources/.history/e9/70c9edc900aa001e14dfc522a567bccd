package ex03_1;

public class Ex111 {
	public static void main(String[] args) {
		
		String[] my_string = new String[6];
		String strs;
		String letter = "f";
		
		my_string[0] = "a";
		my_string[1] = "b";
		my_string[2] = "c";
		my_string[3] = "d";
		my_string[4] = "e";
		my_string[5] = "f";
		
		for (int i = 0; i < my_string.length; i++) {
			if (my_string[i].toString() != letter) {
				strs += my_string[i];
				System.out.println(strs);
			}
			
		}
	}

}
