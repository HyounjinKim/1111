package ex01;

import java.util.Iterator;

public class ex02 {
	
	public static void main(String[] args) {
		
		int a = 10;		
		System.out.println("a = " + a);
		
		int b = 20;		
		System.out.println("a + b = "+ a + b);
		System.out.println("a + b = "+ (a + b));
		
		String c = "a";
		String d = "b";
		System.out.println(c+d);
		
		for(int i=2; i<10; i++) {
			for(int j=1; j<10; j++) {
				System.out.println(i*j);
			}
		}
	}
}
